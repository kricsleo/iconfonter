{
  "name": "iconfonter",
  "type": "module",
  "version": "1.0.2",
  "packageManager": "pnpm@9.10.0",
  "description": "",
  "author": "kricsleo <kricsleo@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/kricsleo/iconfonter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kricsleo/iconfonter.git"
  },
  "bugs": "https://github.com/kricsleo/iconfonter/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "iconfonter": "bin/iconfonter.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint . --fix",
    "prepublishOnly": "nr build",
    "release": "npm run build && bumpp && npm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "picocolors": "^1.1.0",
    "svgo": "^3.3.2"
  },
  "peerDependencies": {
    "jiti": "*"
  },
  "peerDependenciesMeta": {
    "jiti": {
      "optional": true
    }
  },
  "devDependencies": {
    "bumpp": "^9.5.2",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "unbuild": "^2.0.0"
  }
}
